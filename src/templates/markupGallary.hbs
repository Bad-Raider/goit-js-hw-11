{{#each this}}
    <a class="gallery__item" href="{{largeImageURL}}">
    <img  class="gallery__image" src="{{webformatURL}}" alt="" loading="lazy" />
        <div class="info">
            <p class="info-item">
                <b>Likes:</b>
                {{likes}}
            </p>
            <p class="info-item">
                <b>Views:</b>
                {{views}}
            </p>
            <p class="info-item">
                <b>Comments:</b>
                {{comments}}
            </p>
            <p class="info-item">
                <b>Downloads:</b>
                {{downloads}}
            </p>
        </div>
    </a>
{{/each}}
    
 